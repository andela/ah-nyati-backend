{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "server.js",
  "scripts": {
    "build": "babel src -d lib",
    "pretest": "npm run undo:migrate && npm run migrate && npm run seed",
    "test": "NODE_ENV=test nyc mocha --timeout 100000 --require @babel/register --exit",
    "tes": "NODE_ENV=test nyc mocha --timeout 50000 --require @babel/register ./test/article.test.js --exit",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "prettier:fix": "prettier --write \"./**/*.js\"",
    "prettier:check": "prettier --check \"./**/*.js\"",
    "start": "npm run build && node lib/server",
    "start:dev": "nodemon --exec babel-node src/server",
    "migrate": "sequelize db:migrate",
    "undo:migrate": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all"
  },
  "nyc": {
    "exclude": [
      "src/helpers/mail/*.js",
      "src/server.js",
      "src/config/social_config.js"
    ]
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^6.0.0",
    "express-session": "^1.15.6",
    "express-validator": "^6.1.1",
    "file-type": "^12.0.1",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "mongoose": "^5.2.2",
    "mongoose-unique-validator": "^2.0.1",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "nock": "^10.0.6",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.2.1",
    "nodemailer-sendgrid": "^1.0.3",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.11.0",
    "pg-connection-string": "^2.1.0",
    "pg-hstore": "^2.3.3",
    "pusher": "^2.2.1",
    "querystring": "^0.2.0",
    "randomstring": "^1.1.5",
    "request": "^2.87.0",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0",
    "slugify": "^1.3.4",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.7",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "faker": "^4.1.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
}
